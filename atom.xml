---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>CouchDBX</title>
 <link href="http://jongretar.github.com/couchdbx/atom.xml" rel="self"/>
 <link href="http://jongretar.github.com/couchdbx/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://jongretar.github.com/couchdbx/</id>
 <author>
   <name>Jon Gretar Borgthorsson</name>
   <email>jongretar@jongretar.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://jongretar.github.com/couchdbx{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://jongretar.github.com/couchdbx{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>